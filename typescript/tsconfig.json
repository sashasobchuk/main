{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "preserveWatchOutput": true,
    "target": "ES2022",
    "sourceMap": true,
    "incremental": true,
    "lib": [
      "ES2022.Array"
    ]
  }
}
